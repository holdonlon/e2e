<suite name="Parallel Tests" verbose="1" thread-count="4" parallel="tests">
    <tests>
        <test name="Windows + Chrome Test">
        <parameters>
        <parameter name="platform" value="WIN8_1" />
        <parameter name="remoteBrowser" value="chrome"/>
        </parameters>
        <classes>
        <class name="com.google.GoogleSearchTest" />
        </classes>
        </test>
        <test name="Windows + FireFox Test">
            <parameters>
                <parameter name="platform" value="WIN_1" />
                </parameter name="">
            </parameters>
            <packages>
                <package name="com.google">
                </package>
            </packages>
            <!--classes>
                <class name="com.google.SearchGoogleTest" />
                <class name="com.google.SelectTest" />
                <class name="com.google.DragandDrop" />
            </classes>-->
        </test>
    </tests>
</suite>